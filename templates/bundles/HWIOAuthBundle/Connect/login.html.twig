{% extends '@HWIOAuth/layout.html.twig' %}

{% block hwi_oauth_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in hwi_oauth_resource_owners() %}
    <a href="{{ hwi_oauth_login_url(owner) }}"><img src="https://static.makeuseof.com/wp-content/uploads/2015/06/Sign-in-with-Google.png" class="center"></a> <br />
    {% endfor %}
{% endblock hwi_oauth_content %}
